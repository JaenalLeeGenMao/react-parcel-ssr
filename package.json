{
  "name": "parcel-react-ssr",
  "version": "1.0.0",
  "description": "Example of SSR with React and ParcelJS",
  "main": "index.js",
  "keywords": [
    "parcel",
    "react",
    "ssr",
    "babel7"
  ],
  "author": "Jaenal Leegenmao <jaenal.leegenmao@gmail.com.com>",
  "license": "MIT",
  "repository": "https://github.com/jaenalleegenmao/react-parcel-ssr",
  "scripts": {
    "dev": "yarn generate-imported-component && parcel src/index.html",
    "ssr": "yarn build && yarn serve",
    "build": "rimraf dist && yarn generate-imported-component && yarn build-client && yarn build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build src/index.html --no-source-maps -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build server/index.js --no-source-maps -d dist/server --public-url /dist --target=node8",
    "generate-imported-component": "imported-components src src/imported.js",
    "serve": "node dist/server",
    "clear": "rimraf .cache dist"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "node-cache": "^5.0.2",
    "react": "^16.12.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-imported-component": "^6.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.12.4",
    "react-hot-loader": "^4.12.18",
    "rimraf": "^2.6.2"
  }
}
