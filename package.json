{
  "name": "parcel-react-ssr",
  "version": "0.4.0",
  "description": "Example of SSR with React and ParcelJS",
  "main": "index.js",
  "keywords": [
    "parcel",
    "react",
    "ssr"
  ],
  "author": "Benoit Tremblay <benoit@reactivestack.com>",
  "license": "MIT",
  "repository": "reactivestack/parcel-react-ssr",
  "scripts": {
    "dev": "npm run generate-imported-component && parcel src/index.html",
    "build": "rimraf dist && npm run generate-imported-component && npm run build-client && npm run build-server",
    "build-client": "cross-env BABEL_ENV=client parcel build src/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target=node6",
    "generate-imported-component": "imported-components src src/imported.js",
    "serve": "node dist/server"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-imported-component": "^4.0.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.1",
    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2"
  }
}
